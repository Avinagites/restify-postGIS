  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "dependencies": {
    "config": "0.4.x",
    "requireindex": "1.0.x",
    "restify": "2.5.x",
    "mongojs": "~0.10.0",
    "split": "~0.3.0"
  },
  "devDependencies": {
    "request": "2.24.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanj/restify-postGIS.git"
  },
  "scripts": {
